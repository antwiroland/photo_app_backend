{
    "name": "photo-backend",
    "version": "1.0.0",
    "description": "Backend service for photo gallery app (uploads to S3, stores metadata in RDS).",
    "main": "dist/server.js",
    "scripts": {
        "dev": "ts-node-dev --respawn --transpile-only src/server.ts",
        "build": "tsc",
        "start": "node dist/server.js"
    },
    "keywords": [
        "express",
        "aws",
        "s3",
        "rds",
        "photo-gallery"
    ],
    "license": "ISC",
    "type": "commonjs",
    "dependencies": {
        "@aws-sdk/client-s3": "^3.700.0",
        "@aws-sdk/s3-request-presigner": "^3.700.0",
        "dotenv": "^16.3.1",
        "express": "^4.18.2",
        "multer": "^1.4.5-lts.1",
        "pg": "^8.11.1"
    },
    "devDependencies": {
        "@types/express": "^4.17.21",
        "@types/multer": "^1.4.7",
        "@types/node": "^20.5.7",
        "@types/pg": "^8.15.5",
        "ts-node-dev": "^2.0.0",
        "typescript": "^5.3.3"
    }
}
